{
  	"name": "geode",
  	"displayName": "Geode",
  	"description": "Utilities for the Geode Geometry Dash modding framework",
  	"version": "0.0.1",
  	"engines": {
  	  	"vscode": "^1.72.0"
  	},
  	"categories": [
  	  	"Other"
  	],
  	"activationEvents": [
  	  	"*"
  	],
  	"main": "./dist/extension.js",
  	"contributes": {
  	  	"commands": [
			{
				"title": "Geode: Launch Geometry Dash",
				"command": "geode.launchGD"
			}
		],
  	  	"configuration": {
  	  	  	"title": "Geode",
  	  	  	"properties": {
  	  	  	  	"geode.geodeCliPath": {
  	  	  	  	  	"type": "string",
  	  	  	  	  	"default": "",
  	  	  	  	  	"description": "Path to the Geode CLI executable"
  	  	  	  	},
  	  	  	  	"geode.geodeSdkPath": {
  	  	  	  	  	"type": "string",
  	  	  	  	  	"default": "",
  	  	  	  	  	"description": "Path to the Geode SDK"
  	  	  	  	}
  	  	  	}
  	  	},
  	  	"jsonValidation": [
  	  	  	{
  	  	  	  	"fileMatch": "mod.json",
  	  	  	  	"url": "./src/mod-info-validation.json"
  	  	  	}
  	  	]
  	},
  	"scripts": {
  	  	"vscode:prepublish": "npm run package",
  	  	"compile": "webpack",
  	  	"watch": "webpack --watch",
  	  	"package": "webpack --mode production --devtool hidden-source-map",
  	  	"compile-tests": "tsc -p . --outDir out",
  	  	"watch-tests": "tsc -p . -w --outDir out",
  	  	"pretest": "npm run compile-tests && npm run compile && npm run lint",
  	  	"lint": "eslint src --ext ts"
  	},
  	"devDependencies": {
  	  	"@types/glob": "^7.2.0",
  	  	"@types/mocha": "^9.1.1",
  	  	"@types/node": "16.x",
  	  	"@types/semver": "^7.3.13",
  	  	"@types/vscode": "^1.72.0",
  	  	"@typescript-eslint/eslint-plugin": "^5.27.0",
  	  	"@typescript-eslint/parser": "^5.27.0",
  	  	"@vscode/test-electron": "^2.1.3",
  	  	"eslint": "^8.16.0",
  	  	"glob": "^8.0.3",
  	  	"mocha": "^10.0.0",
  	  	"ts-loader": "^9.3.0",
  	  	"typescript": "^4.7.2",
  	  	"webpack": "^5.72.1",
  	  	"webpack-cli": "^4.9.2"
  	},
  	"dependencies": {
  	  	"semver": "^7.3.8"
  	}
}
